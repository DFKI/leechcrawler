<?xml version="1.0" encoding="UTF-8"?>
<mime-info>
	<mime-type type="application/wikipedia+xml">
		<sub-class-of type="application/xml" />
		<acronym>wikipedia</acronym>
		<_comment>Wikipedia dump xml file</_comment>
		<root-XML localName="mediawiki" namespaceURI="http://www.mediawiki.org/xml/export-*/" />
		<glob pattern="*.xml" />
		<magic priority="50">
			<match value="mediawiki xmlns=" type="string" offset="1" />
		</magic>
	</mime-type>
	
	
	
	<mime-type type="application/leechUrlList">
		<sub-class-of type="text/plain" />
		<acronym>leechUrlList</acronym>
		<_comment>plain text file with a list of Urls that should be crawled</_comment>
		<glob pattern="*.urlList" />
	</mime-type>
	
	
</mime-info>